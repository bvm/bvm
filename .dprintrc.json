{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "incremental": true,
  "includes": ["**/*.{json,md,rs}"],
  "excludes": [
    "**/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.7.0.wasm",
    "https://plugins.dprint.dev/markdown-0.4.1.wasm",
    "https://plugins.dprint.dev/rustfmt-0.3.0.wasm"
  ]
}
